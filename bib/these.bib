@techreport{tonoyan_subjective_2020,
	type = {preprint},
	title = {Subjective time is predicted by local and early visual processing},
	url = {http://biorxiv.org/lookup/doi/10.1101/2020.10.30.362038},
	abstract = {ABSTRACT
