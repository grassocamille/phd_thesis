	pages = {912--928},
	file = {Strijkers et al. - 2010 - Tracking Lexical Access in Speech Production Elec.pdf:/Users/camillegrasso/Zotero/storage/R5WI9ZZY/Strijkers et al. - 2010 - Tracking Lexical Access in Speech Production Elec.pdf:application/pdf;Strijkers et al. - 2010 - Tracking Lexical Access in Speech Production Elec.pdf:/Users/camillegrasso/Zotero/storage/YRJ95ZF7/Strijkers et al. - 2010 - Tracking Lexical Access in Speech Production Elec.pdf:application/pdf},
