
@article{stocker_next_2019,
	title = {“{Next} {Wednesday}’s {Meeting} has been {Moved} {Forward} {Two} {Days}”: {The} {Time}-{Perspective} {Question} is {Ambiguous} in {Swiss} {German}, but not in {Standard} {German}},
