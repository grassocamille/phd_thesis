	url = {https://econtent.hogrefe.com/doi/10.1024/1421-0185/a000220},
	doi = {10.1024/1421-0185/a000220},
	abstract = {If time is conceived as a river, we either have the perspective of seeing ourselves as moving downstream toward the future (e.g., “We are approaching the future”, called ego moving), or we have the perspective of the future as moving upstream toward us (e.g., “The future is approaching”, called time moving). Most ego- and time-moving studies have been conducted by using an English ego/time-moving ambiguous question to implicitly measure a person’s current time perspective (ego or time moving). In the current study, we replicate previous findings that the time-perspective question is not ambiguous in (Standard) German, but show as a new finding that it is ambiguous in Swiss German, and we attribute this to different normative uses in these two German variants. Our study is (to our knowledge) the first to investigate dialectal differences in relation to cognitive processing of ego-/timemoving metaphors. Psychological and linguistic implications are discussed.},
	language = {en},
	number = {1-2},
	urldate = {2021-10-13},
	journal = {Swiss Journal of Psychology},
	author = {Stocker, Kurt and Hartmann, Matthias},
	month = apr,
	year = {2019},
	pages = {61--67},
	file = {Stocker et Hartmann - 2019 - “Next Wednesday’s Meeting has been Moved Forward T.pdf:/Users/camillegrasso/Zotero/storage/MMN2FLH6/Stocker et Hartmann - 2019 - “Next Wednesday’s Meeting has been Moved Forward T.pdf:application/pdf},
}

